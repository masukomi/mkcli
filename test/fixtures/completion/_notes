#compdef notes
local curcontext="$curcontext" state line ret=1
_arguments -C \
  "1:cmd:->cmds" \
  "*::arg:->args" && ret=0
case $state in
  (cmds)
    _values "commands" \
      "add[Add a note]" \
      "del[Delete a note]" \
      "list[List notes]" \
      "show[Show a note]" \
      "edit[Edit a note]"
    ret=0
_arguments \
  "(-f --file)"{-f=,--file=}"[File completion]:file:_files" \
  "(-d --directory)"{-d=,--directory=}"[Directory completion]:directory:_directories" \
  "(-u --url)"{-u=,--url=}"[URL completion]:url:_urls" \
  "(-h --help)"{-h,--help}"[Display help and exit]" \
  "--version[Print version and exit]" \
  && return 0;
    ;;
  (args)
    ;;
esac
return ret;
